data_dir: /var/lib/vector
sources:
  syslog:
    type: file
    include: 
      - /var/log/syslog
transforms:
  remap:
    inputs: 
      - syslog
    type: remap
    source: |
            . = parse_syslog!(.message)
sinks:
  result:
    type: file
    inputs:
      - syslog
    compression: none
    encoding: null
    healthcheck: null
    path: /tmp/vector-%Y-%m-%d.log